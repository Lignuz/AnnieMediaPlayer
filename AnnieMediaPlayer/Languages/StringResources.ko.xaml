    <sys:String x:Key="Text.Shortcut.ColumnHeader.Description">설명</sys:String>
    <sys:String x:Key="Text.Shortcut.F5.Shortcut">F5</sys:String>
    <sys:String x:Key="Text.Shortcut.F5.Action">환경설정 열기</sys:String>
    <sys:String x:Key="Text.Shortcut.F5.Description">프로그램의 환경설정 창을 엽니다.</sys:String>
    <sys:String x:Key="Text.Shortcut.F4.Shortcut">F4</sys:String>
