<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="Text.AppTitle">Annie Media Player</sys:String>
    <sys:String x:Key="Text.Open">Open</sys:String>
    <sys:String x:Key="Text.Play">Play</sys:String>
    <sys:String x:Key="Text.Pause">Pause</sys:String>
    <sys:String x:Key="Text.Stop">Stop</sys:String>
    <sys:String x:Key="Text.Sec">Sec</sys:String>
    <sys:String x:Key="Text.Speed">Speed</sys:String>
    <sys:String x:Key="Text.Frame">Frame</sys:String>
    <sys:String x:Key="Text.Error">Error</sys:String>

    <sys:String x:Key="Text.OpenDialogTitle">Open Video File</sys:String>
    <sys:String x:Key="Text.OpenDialogFilter">Video Files (*.mp4;*.avi;*.mov;*.mkv;*.wmv)|*.mp4;*.avi;*.mov;*.mkv;*.wmv|All Files (*.*)|*.*</sys:String>
    <sys:String x:Key="Text.OpenVideoFailed">Failed to open video</sys:String>

    <sys:String x:Key="Text.Settings">Settings</sys:String>
    <sys:String x:Key="Text.General">General</sys:String>
    <sys:String x:Key="Text.Advanced">Advanced</sys:String>

</ResourceDictionary>
